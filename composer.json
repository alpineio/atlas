{
    "name": "alpineio/atlas",
    "description": "WordPress custom post type manager",
    "authors": [
        {
            "name": "Morgan O'Neal",
            "email": "moneal@alpine.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlpineIO\\Atlas\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "alpineio/wp-develop",
                "version": "4.5.2",
                "dist": {
                    "url": "https://github.com/nacin/wp-develop/archive/4.5.2.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-4": {
                        "AlpineIO\\WPDevelop\\": "src/",
                        "AlpineIO\\WPDevelopTests\\": "tests/"
                    }
                }
            }
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "nesbot/carbon": "^1.21",
        "illuminate/support": "^5.2",
        "phpdocumentor/reflection-docblock": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "alpineio/wp-develop": "^4.5",
        "mockery/mockery": "^0.9.5",
        "fzaninotto/faker": "^1.6"
    }
}
